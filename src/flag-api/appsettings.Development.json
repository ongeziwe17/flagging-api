{
    "ConnectionStrings": {
        "Default": "Server=${SQL_SERVER_HOST},${SQL_SERVER_PORT};Database=${DATABASE_NAME};User Id=${SQL_SERVER_USER};Password=${SQL_SERVER_PASSWORD};TrustServerCertificate=True;Encrypt=False",
        "Redis": "${REDIS_HOST}:${REDIS_PORT},password=${REDIS_PASSWORD}"
    },
    "Admin": {
        "BootstrapAdminKey": "${ADMIN_KEY}"
    },

    "AllowedHosts": "*"
}
